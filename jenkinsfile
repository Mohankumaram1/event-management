pipeline {
   agent { label 'jenkins-agent'}
   tools{
     jdk 'java17'
     maven 'maven3'
   }
stages("cleanup workspace")
steps{
cleanws()
}
}
stage("checkout from SCM"){
steps{
git branch: 'main' ,credintialsId: 'github',url: 'git@github.com:Mohankumaram1/event-management.git'
}
}
stage("build application"){
steps { 
sh "mvn clean package"
}
}
stage("test application"){
steps {
sh "mvn test"
}
}
}
}
